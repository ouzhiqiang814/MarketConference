/* pages/meetingReport/meetingReport.wxss */

.meet-image {
  width: 100%;
  height: 316rpx;
  display: block;
}

.meetingReport-content {
  width: 80%;
  margin: 0 auto;
  margin-top: -120rpx;
  z-index: 10;
  left: 0;
  right: 0;
}

.meetingReport-box {
  background-color: #fff;
  box-shadow: 0px 0px 2px 3px #e9e9ea;
  width: 100%;
  border-radius: 5px;
  padding: 20px 10px 20px 10px;
}

.meetingReport-box-top {
  color: #343434;
  text-align: center;
  font-size: 18px;
  font-weight: 600;
}

.meetingReport-mid {
  display: flex;
  justify-content: space-around;
  margin-top: 15px;
}

.meetingReport-mid-top-text {
  display: flex;
  justify-content: center;
}

.meetingReport-mid-top-text_1 {
  font-size: 24px;
  color: #fe160f;
}

.meetingReport-mid-top-text_2 {
  font-size: 14px;
  color: #fe160f;
  line-height: 45px;
  margin-left: 3px;
}

.meetingReport-mid-bot-text {
  font-size: 14px;
  text-align: center;
  color: #666;
  margin-top: -12px;
}

.meetingReport-mid-mid {
  width: 1px;
  background-color: #ddd;
}

.meetingReport-bottom {
  margin-top: 15px;
  display: flex;
  justify-content: space-around;
}

.meetingReport-fnbox {
  position: relative;
  width: 140rpx;
  height: 57px;
}

.meetingReport-fnbox image {
  width: 100%;
  height: 100%;
}

.meetingReport-fnbox-top {
  display: flex;
  position: absolute;
  top: 0px;
  color: #fff;
  left: 0;
  right: 0;
  margin: 0 auto;
  justify-content: center;
}

.meetingReport-fnbox-top_1 {
  font-size: 20px;
}

.meetingReport-fnbox-top_2 {
  font-size: 14px;
  line-height: 40px;
  margin-left: 2px;
}

.meetingReport-fnbox-bot {
  color: #fff;
  font-size: 12px;
  position: absolute;
  bottom: 6px;
  text-align: center;
  width: 100%;
}

.meetingReport-content-bot {
  width: 80%;
  margin: 0 auto;
  margin-top: 20px;
  z-index: 10;
  left: 0;
  right: 0;
}
.meetingReport-image{
  width: 100%;
  height: 125px;
  margin-top: 15px;
}
.meetingReport-image image{
  width: 100%;
  height: 100%;
}
.meetingReport-text{
  font-size: 14px;
  margin-top: 15px;
  color: #666;
}
.meetingReport-span{
  width: 100%;
  margin-top: 30px;
  height: 50px;
}